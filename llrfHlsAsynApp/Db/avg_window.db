record(waveform, "$(DEVICE):AVGWND$(WND)")
{
    field(PINI, "YES")
    field(DESC, "average window")
    field(DTYP, "asynFloat64ArrayOut")
    field(INP,  "@asyn($(PORT),0) avg_window$(WND)")
    field(SCAN, "Passive")
    field(FTVL, "DOUBLE")
    field(NELM, "$(NELM)")
    info(autosaveFields, "VAL")
}

