record(bo, "$(DEVICE):OPMODE")
{
    field(PINI, "YES")
    field(DESC, "OP mode bkwd_cmtb/adaptive")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) op_mode")
    field(SCAN, "Passive")
    field(OMSL, "supervisory")
    field(ZNAM, "BKWD_CMPTB")
    field(ONAM, "Adaptive")
    info(autosaveFields, "VAL")
}

