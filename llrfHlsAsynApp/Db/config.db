#
record(ao, "$(DEVICE):REFPOC")
{
    field(PINI, "YES")
    field(DESC, "phase offset correction for reference")
    field(DTYP, "asynFloat64")
    field(OUT,  "@asyn($(PORT),0) p_ref_offset")
    field(OMSL, "superviaory")
    field(SCAN, "Passive")
    field(PREC, "3")
    field(HHSV, "MAJOR")
    field(HSV,  "MINOR")
    field(LSV,  "MINOR")
    field(LLSV, "MAJOR")
    info(autosaveFields, "VAL LOLO LOW HIGH HIHI")
}

