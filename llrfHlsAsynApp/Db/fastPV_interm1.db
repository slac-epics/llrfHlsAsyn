record(ai, "$(DEVICE):$(TS_N)W$(WND)CH$(CH)_PHASE")
{
    field(DESC, "phase for $(TS_N) wnd $(WND) ch $(CH)")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),0) p_br_$(TS_ARG)w$(WND)ch$(CH)")
    field(SCAN, "I/O Intr")
    field(PREC, "3")
    field(TSE,   "-2")
}

record(ai, "$(DEVICE):$(TS_N)W$(WND)CH$(CH)_AMPL")
{
    field(DESC, "ampl for $(TS_N) wnd $(WND) ch $(CH)")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),0) a_br_$(TS_ARG)w$(WND)ch$(CH)")
    field(SCAN, "I/O Intr")
    field(PREC, "3")
    field(TSE,  "-2")
}


