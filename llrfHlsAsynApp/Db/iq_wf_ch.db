record(waveform, "$(DEVICE):IWF$(CH)")
{
    field(DESC, "I waveform for channel $(CH)")
    field(DTYP, "asynFloat64ArrayIn")
    field(INP,  "@asyn($(PORT),0) i_wf_ch$(CH)")
    field(SCAN, "I/O Intr")
    field(FTVL, "DOUBLE")
    field(NELM, "$(NELM)")
}

