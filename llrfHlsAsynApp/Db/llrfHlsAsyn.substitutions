file Bsa.db
{
  pattern {  DEVICE      ,  ATRB             , LNK      ,  EGU     ,  HOPR,  LOPR  }
          { "$(DEV)"     , "FB_FAST_PDES"    , ""       , "degrees", "180.", "-180."}
          { "$(DEV)"     , "FB_FAST_ADES"    , ""       , ""       ,     "",    "0."}
          { "$(DEV)"     , "FB_FAST_PSET"    , ""       , "degreees","180.", "-180."}
