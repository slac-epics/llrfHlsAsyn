file iq_wf_ch.db
{
    pattern { CH, CH_N, NELM,  DEVICE  }
            {  0,   00, 4096, "$(DEV)" }
            {  1,   01, 4096, "$(DEV)" }
            {  2,   02, 4096, "$(DEV)" }
            {  3,   03, 4096, "$(DEV)" }
            {  4,   04, 4096, "$(DEV)" }
            {  5,   05, 4096, "$(DEV)" }
            {  6,   06, 4096, "$(DEV)" }
            {  7,   07, 4096, "$(DEV)" }
            {  8,   08, 4096, "$(DEV)" }
            {  9,   09, 4096, "$(DEV)" }
}

file avg_window.db
{
    pattern { NELM,  DEVICE  }
            { 4096, "$(DEV)" }
}



