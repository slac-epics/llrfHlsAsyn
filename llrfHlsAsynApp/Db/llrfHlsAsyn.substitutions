###########################
# PDES/ADES configuration #
###########################
file rfTSmux.db
{
    pattern {  DEVICE  }
            { "$(DEV)" }
}



file waveformTimeAxis.db
{
    pattern {   DEVICE  , DESC                         ,  SIZE ,  FREQ   }
            {  "$(DEV)" , "HLS waveform time axis"     , "4096" , "357"  }
}


#############################
# Destination Aware Trigger #
#############################

file DestTrig_kind.db
{
    pattern   {   DEVICE,        TRG,       TRG_STR } 
              { "$(DEV)",     "LLRF",        "llrf" }
              { "$(DEV)",      "RTM",         "rtm" }
              { "$(DEV)",      "MOD",         "mod" }
              { "$(DEV)",      "SSA",         "ssa" }
}

file DestTrig_accl_stdby.db
{
    pattern   {   DEVICE,        TRG  }
              { "$(DEV)",      "ACCL" }
              { "$(DEV)",     "STDBY" }
}

file DestTrig_dest.db
{
    pattern {     DEVICE,            TPR_NAME,      DEST,      CH0,      CH1  }
            {   "$(DEV)",     "$(TPR_PREFIX)",   "DEST0",     "00",      "01" }
            {   "$(DEV)",     "$(TPR_PREFIX)",   "DEST1",     "02",      "03" }
            {   "$(DEV)",     "$(TPR_PREFIX)",   "DEST2",     "04",      "05" }
}

file DestTrig_ch.db
{
    pattern {    DEVICE,            TPR_NAME,     DEST,       TRG,      CH   }
            {  "$(DEV)",     "$(TPR_PREFIX)",  "DEST0",    "ACCL",    "00"   }
            {  "$(DEV)",     "$(TPR_PREFIX)",  "DEST0",   "STDBY",    "01"   }
            {  "$(DEV)",     "$(TPR_PREFIX)",  "DEST1",    "ACCL",    "02"   }
            {  "$(DEV)",     "$(TPR_PREFIX)",  "DEST1",   "STDBY",    "03"   }
            {  "$(DEV)",     "$(TPR_PREFIX)",  "DEST2",    "ACCL",    "04"   }
            {  "$(DEV)",     "$(TPR_PREFIX)",  "DEST2",   "STDBY",    "05"   }
}


file DestTrig_evcode.db
{
    pattern {    DEVICE,            TPR_NAME,      DEST,      TRG,      CH   }
            {  "$(DEV)",     "$(TPR_PREFIX)",   "DEST0",   "ACCL",     "00"  }
            {  "$(DEV)",     "$(TPR_PREFIX)",   "DEST0",  "STDBY",     "01"  }
            {  "$(DEV)",     "$(TPR_PREFIX)",   "DEST1",   "ACCL",     "02"  }
            {  "$(DEV)",     "$(TPR_PREFIX)",   "DEST1",  "STDBY",     "03"  }
            {  "$(DEV)",     "$(TPR_PREFIX)",   "DEST2",   "ACCL",     "04"  }
            {  "$(DEV)",     "$(TPR_PREFIX)",   "DEST2",  "STDBY",     "05"  }
}

file DestTrig_common_evcode.db
{
		pattern {    DEVICE,            TPR_NAME,       TRG,      CH   }
				{  "$(DEV)",     "$(TPR_PREFIX)",  "DAQMUX",     "06"  }
				{  "$(DEV)",     "$(TPR_PREFIX)",  "ACALGN",     "07"  }
}

