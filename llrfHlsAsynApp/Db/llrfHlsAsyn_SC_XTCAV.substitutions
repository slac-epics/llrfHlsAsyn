file bsa.db
{
    pattern {       DEV,      PORT,     BSAKEY,           SECN       }
             { "$(DEV)", "bsaPort",     "PACT",     "FB_FAST_PACT"   }
             { "$(DEV)", "bsaPort",     "AACT",     "FB_FAST_AACT"   }
             { "$(DEV)", "bsaPort",     "PDES",     "FB_FAST_PDES"   }
             { "$(DEV)", "bsaPort",     "ADES",     "FB_FAST_ADES"   }
             { "$(DEV)", "bsaPort",     "PSET",     "FB_FAST_PSET"   }
             { "$(DEV)", "bsaPort",     "ASET",     "FB_FAST_ASET"   }
             { "$(DEV)", "bsaPort",     "PCH0",     "W0C0_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH0",     "W0C0_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH1",     "W0C1_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH1",     "W0C1_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH2",     "W0C2_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH2",     "W0C2_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH3",     "W0C3_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH3",     "W0C3_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH4",     "W0C4_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH5",     "W0C4_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH5",     "W0C5_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH5",     "W0C5_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH6",     "W0C6_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH6",     "W0C6_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH7",     "W0C7_FAST_PACT" }
             { "$(DEV)", "bsaPort",     "ACH7",     "W0C7_FAST_AACT" }
             { "$(DEV)", "bsaPort",     "PCH8",     "W0C8_FAST_PACT" }
