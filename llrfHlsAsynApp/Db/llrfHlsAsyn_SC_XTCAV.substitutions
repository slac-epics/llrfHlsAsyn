file fastPV.db
{
    pattern { TS, TS_N, TS_PV, TS_ARG,  DEVICE  }
            { NT,   NT,    NT,     NT, "$(DEV)" } 
            {  0,   00,  TS00,     t0, "$(DEV)" }
            {  1,   01,  TS01,     t1, "$(DEV)" }
            {  2,   02,  TS02,     t2, "$(DEV)" }
            {  3,   03,  TS03,     t3, "$(DEV)" }
            {  4,   04,  TS04,     t4, "$(DEV)" }
            {  5,   05,  TS05,     t5, "$(DEV)" }
            {  6,   06,  TS06,     t6, "$(DEV)" }
            {  7,   07,  TS07,     t7, "$(DEV)" }
            {  8,   08,  TS08,     t8, "$(DEV)" }
            {  9,   09,  TS09,     t9, "$(DEV)" }
#
            { 10,   10,  TS10,    t10, "$(DEV)" }
            { 11,   11,  TS11,    t11, "$(DEV)" }
            { 12,   12,  TS12,    t12, "$(DEV)" }
            { 13,   13,  TS13,    t13, "$(DEV)" }
            { 14,   14,  TS14,    t14, "$(DEV)" }
            { 15,   15,  TS15,    t15, "$(DEV)" }
            { 16,   16,  TS16,    t16, "$(DEV)" }
            { 17,   17,  TS17,    t17, "$(DEV)" }
}


