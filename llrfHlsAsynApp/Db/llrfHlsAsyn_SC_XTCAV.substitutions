file scBsa_linconv.db
{
    pattern  {   DEVICE,     PARAM,      SECN,          SECN_A,          SECN_B         , SECN_C,    DESC }
              { "$(DEV)",    "pact",    "PACT", "FB_FAST_PACT",  "FB_FAST_PDES",  "FB_FAST_PSET",  "phase"}
              { "$(DEV)",    "aact"     "AACT", "FB_FAST_AACT",  "FB_FAST_ADES",  "FB_FAST_ASET",  "amplitude"}
}


file scBsa_linconv_ch.db
{
