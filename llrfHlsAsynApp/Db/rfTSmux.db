{
    field(DESC, "Calculation for T0 AREQ")
    field(CALC, "A+B")
    field(INPA, "$(DEVICE):AREQ")
    field(INPB, "$(DEVICE):T0_AREQ_OFFSET")
    field(OUT,  "$(DEVICE):ADES00 PP")
    field(ASG,  "Internal")
}

