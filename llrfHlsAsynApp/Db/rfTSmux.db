record(ao, "$(DEVICE):PDES")
{
    field(PINI, "YES")
    field(DESC, "PDES master")
    field(SCAN, "Passive")
    field(PREC, "3")
    field(EGU,  "degrees")
    field(FLNK, "$(DEVICE):PDES_FANOUT PP")
    field(DRVL, "-180")
    field(DRVH, "180")
    field(LOPR, "-180")
    field(HOPR, "180")
    field(HHSV, "MAJOR")
    field(HSV,  "MINOR")
    field(LSV,  "MINOR")
    field(LLSV, "MAJOR")
    info(autosaveFields, "VAL LOLO LOW HIGH HIHI")
}

record(fanout, "$(DEVICE):PDES_FANOUT")
{
    field(DESC, "fanout for PDES")
    field(LNK1, "$(DEVICE):T4:PDES_CALC PP")
    field(LNK2, "$(DEVICE):T1:PDES_CALC PP")
    field(LNK3, "$(DEVICE):T2:PDES_CALC PP")
    field(LNK4, "$(DEVICE):T3:PDES_CALC PP")
    field(LNK5, "$(DEVICE):T5:PDES_CALC PP")
    field(LNK6, "$(DEVICE):T6:PDES_CALC PP")
    field(FLNK, "$(DEVICE):PDESTS PP")
    field(ASG, "Internal")
}

record(stringin, "$(DEVICE):PDESTS")
{
  field(DESC, "PDES Time Stamp")
  field(PINI, "YES")
  field(DTYP, "Soft Timestamp")
  field(VAL, "Unknown")
  field(INP, "@%m/%d/%y %H:%M:%S")
  field(ASG, "Internal")
  info(autosaveFields, "VAL")
}

