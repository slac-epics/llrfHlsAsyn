file fastPV.db
{
    pattern { TS, TS_N, TS_PV, TS_ARG,  DEVICE  }
            { NT,   NT,    NT,     NT, "$(DEV)" } 
            {  0,   00,  TS00,     t0, "$(DEV)" }
            {  1,   01,  TS01,     t1, "$(DEV)" }
            {  2,   02,  TS02,     t2, "$(DEV)" }
            {  3,   03,  TS03,     t3, "$(DEV)" }
            {  4,   04,  TS04,     t4, "$(DEV)" }
            {  5,   05,  TS05,     t5, "$(DEV)" }
            {  6,   06,  TS06,     t6, "$(DEV)" }
            {  7,   07,  TS07,     t7, "$(DEV)" }
            {  8,   08,  TS08,     t8, "$(DEV)" }
            {  9,   09,  TS09,     t9, "$(DEV)" }
#
